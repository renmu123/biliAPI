{
  "name": "@renmu/bili-api",
  "version": "1.2.4",
  "type": "module",
  "description": "bilibili 接口的 node 包装库",
  "homepage": "https://github.com/renmu123/biliAPI",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index",
  "typings": "dist/index",
  "exports": {
    ".": {
      "types": {
        "default": "./dist/index"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "rollup --config rollup-dev.config.js -w",
    "build": "rollup --config rollup.config.js",
    "publish": "pnpm run build && npm publish --access=public"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bilibili",
    "api"
  ],
  "author": "renmu123",
  "license": "GPL",
  "dependencies": {
    "axios": "^1.6.5",
    "axios-retry": "^4.0.0",
    "fast-xml-parser": "^4.3.3",
    "p-queue": "^8.0.1",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "@kineticcafe/rollup-plugin-delete": "3.0.0-beta.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "20.10.0",
    "rollup": "^4.9.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
