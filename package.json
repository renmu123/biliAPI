{
  "name": "@renmu/bili-api",
  "version": "1.4.0",
  "type": "module",
  "description": "bilibili 接口的 node 包装库",
  "homepage": "https://github.com/renmu123/biliAPI",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "default": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "pnpm run test && pnpm run typecheck && tsc && cpy src/assets/dm.proto dist/assets --flat",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false",
    "test": "vitest run",
    "release": "pnpm run build && npm publish --access=public"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bilibili",
    "api"
  ],
  "author": "renmu123",
  "license": "GPL",
  "dependencies": {
    "axios": "^1.7.3",
    "axios-retry": "^4.5.0",
    "fast-xml-parser": "^4.4.1",
    "p-queue": "^8.0.1",
    "protobufjs": "^7.3.2",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "22.1.0",
    "cpy-cli": "^5.0.0",
    "rimraf": "^6.0.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
